{
    "name": "api-auto-import",
    "version": "0.0.23",
    "exports": {
        "./vite": {
            "import": {
                "types": "./dist/vite.d.ts",
                "default": "./dist/vite.js"
            },
            "require": {
                "types": "./dist/vite.d.cts",
                "default": "./dist/vite.cjs"
            }
        },
        "./webpack": {
            "import": {
                "types": "./dist/webpack.d.ts",
                "default": "./dist/webpack.js"
            },
            "require": {
                "types": "./dist/webpack.d.cts",
                "default": "./dist/webpack.cjs"
            }
        }
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.11.28"
    },
    "keywords": [
        "plugin",
        "vite",
        "webpack",
        "path"
    ],
    "sideEffects": false,
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "files": [
        "dist",
        "src/template.ts"
    ],
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "fs": "^0.0.1-security",
        "lodash": "^4.17.21",
        "path": "^0.12.7",
        "vite": "^5.1.6"
    }
}